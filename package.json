{
	"name": "fizz-buzz-extension",
	"private": true,
	"displayName": "FizzBuzz Extension",
	"version": "1.0.0",
	"publisher": "Annykom",
	"homepage": "https://github.com/Annykom/fizz-buzz-extension/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/Annykom/fizz-buzz-extension"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:python"
	],
	"contributes": {
		"configuration": {
			"title": "FizzBuzz",
			"properties": {
			  	"fizzbuzz.transforms": {
					"type": "array",
					"order": 1,
					"description": "Set delimiters and function names to rename. Use an input mask: <delimiter>|<function name>",
					"items": {
						"type":"string"
					},
					"default": [
						"3|fizz",
						"5|buzz"
					]
				}
			}
		}
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^5.0.2"
	}
}
